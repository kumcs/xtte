INSERT INTO te.teprj (
  teprj_prj_id,
  teprj_cust_id,
  teprj_rate,
  teprj_curr_id )
VALUES (
  <? value("prj_id") ?>,
  <? value("cust_id") ?>,
  <? value("rate") ?>,
  <? value("curr_id") ?> )
ON CONFLICT (teprj_prj_id) DO
UPDATE SET
  teprj_cust_id=<? value("cust_id") ?>,
  teprj_rate=<? value("rate") ?>,
  teprj_curr_id=<? value("curr_id") ?>;
